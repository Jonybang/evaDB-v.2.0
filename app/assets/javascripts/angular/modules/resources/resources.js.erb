'use strict';

angular.module('app.resources', ['ui.router'])
        .config(['$urlRouterProvider', '$stateProvider', function($urlRouterProvider, $stateProvider) {

            <% @templates_path = 'angular/modules/resources/templates/' %>

            $stateProvider
                    .state('app.resources', {
                        url: '/resources',
                        controller: 'ResourcesIndexCtrl',
                        templateUrl: '<%= asset_path(@templates_path + 'index.html') %>'
                    });
        }]);
